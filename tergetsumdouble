#include <iostream>
#include <vector>
using namespace std;

int main(){
    int array[]={3,46,7,1};
    
    int tergetsum=7;
    int size=5;
    int pairs=0;
    
    for(int i=0; i<size; i++){
        for(int j=i+1; j<size; j++){
            if(array[i]+array[j]==tergetsum){
                pairs++;
            }
         
        }
    }
    cout<<pairs<<endl;
   
    return 0;
}
