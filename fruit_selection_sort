#include <iostream>
#include <vector> 
#include <cstring>
using namespace std ;
void selection_fruit_Sort(char fruit[][60], int n){
    
    for (int i=0; i<n-1;i++){
        //find the min ele
        int min_idx =i;
        for (int j=i+1; j<n; j++){
            if (strcmp(fruit[min_idx], fruit[j])>0){
                min_idx=j;
            }
 
        }
        //place the min ele at the begining
        if (i!=min_idx){
            swap(fruit[i], fruit[min_idx]);
        }
    
    }
    return;
}
int main(){
    char fruit[][60] = {"papaya", "lime", "watermelon", "apple", "mango", "kiwi"};
    int n= sizeof(fruit)/sizeof(fruit[0]);
    selection_fruit_Sort(fruit, n) ;
    for (int i=0; i<n; i++){
        cout<<fruit[i]<<" ";
    }cout<<endl;
    
}



