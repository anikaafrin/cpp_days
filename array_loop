#include <iostream>

using namespace std;

int main()
{
    int array[]={1,2,3,4,5};
    //cout<<sizeof(array)/sizeof(array[0])<<endl;
    int size=sizeof(array)/sizeof(array[0]);
    for (int idx=0; idx<size; idx++){
        cout<<array[idx]<<endl;
        
        
        
    }
    
    for (int ele:array){
        cout<<ele<<endl;
    }
    int index=0;
    while(index<size){
        
        cout<<array[index]<<endl;
        index++;
    }

    return 0;
}
