#include <iostream>
#include<vector>
#include <limits.h>
#include <algorithm>
using namespace std;


int main()
{ 
  int x, y;
  cin>>x>>y;
    int *ptrx= &x;
    int *ptry= &y;
    int result;
    int *ptr_result= &result;
    *ptr_result= *ptrx + *ptry;
    cout<<result<<" "<<*ptr_result<<endl;

    return 0;
}
